{
  "name": "francoism90/mediadb",
  "type": "project",
  "description": "Personal media manager.",
  "keywords": [
    "media",
    "stream",
    "mp4"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "babenkoivan/scout-elasticsearch-driver": "^4.2",
    "beyondcode/laravel-websockets": "^1.8",
    "cyrildewit/eloquent-viewable": "^5.2",
    "elegantweb/sanitizer": "^1.0",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.1",
    "laravel/sanctum": "^2.6",
    "laravel/telescope": "^4.0",
    "laravel/tinker": "^2.0",
    "multicaret/laravel-acquaintances": "^3.3",
    "php-ffmpeg/php-ffmpeg": "^0.16.0",
    "pusher/pusher-php-server": "^4.1",
    "spatie/laravel-activitylog": "^3.16",
    "spatie/laravel-db-snapshots": "^1.7",
    "spatie/laravel-json-api-paginate": "^1.8",
    "spatie/laravel-medialibrary": "^8.7",
    "spatie/laravel-model-status": "^1.10",
    "spatie/laravel-permission": "^3.17",
    "spatie/laravel-query-builder": "^3.2",
    "spatie/laravel-responsecache": "^6.6",
    "spatie/laravel-sluggable": "^2.5",
    "spatie/laravel-tags": "^3.0",
    "spatie/laravel-translatable": "^4.5",
    "vinkla/hashids": "^9.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.7",
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "nunomaduro/larastan": "^0.6.4",
    "phpunit/phpunit": "^9.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan horizon:publish --ansi",
      "@php artisan telescope:publish --ansi",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  }
}
